# glmmTMB for lrgen
lrgen_glmmtmb <- glmmTMB(true_vote_lrgen ~ 
                                 lrgen_cmc * (parliamentary_term + environment_only + procedure + final_vote) +
                                 eu_position_cmc * (parliamentary_term + environment_only + procedure + final_vote) +
                                 galtan_cmc * (parliamentary_term + environment_only + procedure + final_vote) +
                                 lrgen_cmc * parliamentary_term * environment_only +
                                 eu_position_cmc * parliamentary_term * environment_only +
                                 galtan_cmc * parliamentary_term * environment_only +
                                 (1 | mep_unique) +
                                 (1 | vote_id_unique),
                               data = legislative_mlr_centered,
                               family = binomial())
summary(lrgen_glmmtmb)

# Predict probabilities
predictions <- predict(lrgen_glmmtmb, type = "response")
